<h1>QINeS AUTOSAR Purosess Tool</h1>

<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(:session, url: sessions_path) do |f| %>
        <%= f.label :name, 'ユーザ名' %>
        <%= f.text_field :name, class: 'form-control'%>

        <%= f.label :password, 'パスワード' %>
        <%= f.password_field :password,class: 'form-control'%>

        <%= f.submit "Sign in", class: "btn btn-primary col-xs-4" %>
    <% end %>
  </div>
</div>
<div>
  <table class="table table-bordered" id="signup_tb">
    <td>Don't have an account? <%= link_to "Sign up", signup_path, id: "signup_btn" %></td>
  </table>
</div>
